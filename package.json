{
  "name": "devbridge-frontend",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/devbridge/Front-End-Toolkit.git"
  },
  "description": "Starter project",
  "main": "",
  "browserslist": [
    ">0.25%",
    "not op_mini all"
  ],
  "dependencies": {
    "lodash": "4.17.20",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack -p --mode production",
    "stylelint:lint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "eslint:check": "eslint \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devbridge/Front-End-Toolkit/issues"
  },
  "homepage": "https://github.com/devbridge/Front-End-Toolkit",
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.18.6",
    "@svgr/webpack": "6.4.0",
    "autoprefixer": "9.8.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.1.0",
    "check-dependencies": "1.1.0",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.2.1",
    "css-loader": "3.6.0",
    "cssnano": "4.1.10",
    "eslint": "5.14.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-lodash": "5.1.0",
    "eslint-plugin-react": "7.21.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-advanced-variables": "3.0.1",
    "postcss-functions": "3.0.0",
    "postcss-hexrgba": "2.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-responsive-type": "1.0.0",
    "postcss-scss": "2.1.1",
    "precss": "4.0.0",
    "prettier": "2.0.5",
    "sass": "1.55.0",
    "sass-loader": "13.1.0",
    "style-loader": "1.2.1",
    "style-resources-loader": "1.3.3",
    "stylelint": "13.0.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-sass-guidelines": "7.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-declaration-use-variable": "1.7.2",
    "stylelint-order": "4.0.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "stylelint-webpack-plugin": "2.1.0",
    "url-loader": "4.1.0",
    "webpack": "5.74.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,json,md}": [
      "eslint",
      "git add"
    ],
    "./src/**/*.css": [
      "stylelint",
      "git add"
    ],
    "*.{js,jsx,ts,tsx,json,md,css}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
